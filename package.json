    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "next": "12.3.1",
    "next-auth": "^4.12.0",
    "react": "18.2.0",
    "react-countdown": "^2.3.2",
    "react-dom": "18.2.0"
